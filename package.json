{
  "name": "better-news",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "if [ !$NEST_HEROKU ]; then node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main; fi",
    "start": "if [ $NEST_HEROKU ]; then npm run start:nest:prod; elif [ $NG_HEROKU ]; then npm run start:ng:prod; else nx serve; fi",
    "build": "if [ $NEST_HEROKU ]; then npm run build:nest; elif [ $NG_HEROKU ]; then npm run build:ng; else nx build; fi",
    "start:prod": "if [ $NEST_HEROKU ]; then npm run start:nest:prod; elif [ $NG_HEROKU ]; then npm run start:ng:prod; fi",
    "test": "nx test",
    "build:ng": "nx run better-news-app:build -- --prod",
    "build:nest": "nx run api:build",
    "start:nest": "nx run api:serve",
    "start:nest:prod": "node dist/apps/api/main.js",
    "start:ng:prod": "node server.js"
  },
  "engines": {
    "node": "14.17.3",
    "npm": "6.14.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.0",
